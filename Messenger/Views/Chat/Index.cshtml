@model IEnumerable<Messenger.PL.ViewModels.DisplayChatsViewModel>
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

<div class="list-group">
    @foreach (var item in Model)
    {
        <div class="my-10">
            <a href=@("/messege/" + item.ChatId) class="list-group-item list-group-item-action">@item.ChatName</a>
        </div>
    }
</div>